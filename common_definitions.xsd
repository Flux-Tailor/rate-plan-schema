<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
    elementFormDefault="qualified" vc:minVersion="1.0" vc:maxVersion="1.1">
    <!--
=======================================
Schema: common_definitions.xsd  
Author: 
License:
=======================================
-->

    <!--
=======================================
Attributes     
=======================================
-->
    <xs:attribute name="ratePlanURI" type="xs:anyURI">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">a Unified Resource Identifier (URI)for the rate plan to consistently locate the rate plan and its parameters.

draft example: https://iedr.nyserda.ny.gov/rate-plan/90d30878-b022-4c54-87bb-71fe7359aae6</xs:appinfo>
            <xs:documentation xml:lang="en-US">a Unified Resource Identifier (URI)for the rate plan to consistently locate the rate plan and its parameters. This URI can be used to reference the rate plan from for example a customer's service agreement data set.

draft example: https://iedr.nyserda.ny.gov/rate-plan/90d30878-b022-4c54-87bb-71fe7359aae6</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ratePlanModifierURI" type="xs:anyURI">
        <xs:annotation>
            <xs:appinfo xml:lang="en">a unique identifier for the rate plan modifier to consistently identify and access data</xs:appinfo>
            <xs:documentation xml:lang="en">ex: https://iedr.nyserda.ny.gov/rate-plan-modifier/50d30878-b022-4c54-87bb-71fe7359aae6</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup name="crud">
        <xs:attribute ref="isReplacementRecord"/>
        <xs:attribute ref="isDeletedRecord"/>
    </xs:attributeGroup>
    <xs:attribute name="isReplacementRecord" type="xs:boolean">
        <xs:annotation>
            <xs:documentation>The record replaces a record included in a prior data transfer.</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isDeletedRecord" type="xs:boolean">
        <xs:annotation>
            <xs:documentation>The record has been deleted or updated in the source from which it was extracted.</xs:documentation>
        </xs:annotation>
    </xs:attribute>

    <!--
=======================================
Complex Types
=======================================
-->
    <xs:complexType name="ApplicabilityType">
        <xs:sequence>
            <xs:element minOccurs="0" ref="identifiers"/>
            <xs:element minOccurs="0" ref="hasMeasurementKinds"/>
            <xs:element minOccurs="0" ref="modelComponents"/>
            <xs:element ref="profile" minOccurs="0"/>
            <xs:element minOccurs="0" ref="seasonNames">
                <xs:annotation>
                    <xs:appinfo>Applicable season</xs:appinfo>
                    <xs:documentation>Applicable season</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="customRatePlanAttributes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Flexible options for rate plan attributes by which to filter rate plans for applicability - TODO: consider removing as no linking is possible to rate plans and rate plan modifiers</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element ref="customRatePlanAttribute"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DateIntervalType">
        <xs:sequence>
            <xs:element form="qualified" name="startDate" type="xs:date"/>
            <xs:element name="endDate" form="qualified" type="xs:date"
                minOccurs="0"/>
            <xs:element minOccurs="0" name="expectedEndDate" type="xs:date"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DateTimeIntervalType">
        <xs:sequence>
            <xs:element form="qualified" name="startDateTime" type="xs:dateTime"/>
            <xs:element name="endDateTime" form="qualified" type="xs:dateTime"
                minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DocumentationType">
        <xs:sequence>
            <xs:element ref="utilityInfo"/>
            <xs:element ref="commodity"/>
            <xs:element ref="dataSource"/>
            <xs:element ref="mediaType">
                <xs:annotation>
                    <xs:appinfo>media type of the source document</xs:appinfo>
                    <xs:documentation>media type for the document as registered by IANA, enumeration contains subset allowed.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sourceURL" type="xs:anyURI">
                <xs:annotation>
                    <xs:documentation>URL from which official source tariff documentation can be downloaded</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dataCreationMethod">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Manual"/>
                        <xs:enumeration value="AutomatedDataExtraction"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dataDownloadURL" type="xs:anyURI" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>URL from which official structured data was downloaded</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="effectivePeriod"/>
            <xs:element name="datePublished" type="xs:date">
                <xs:annotation>
                    <xs:documentation>The date on which tariff documentation was published in official sources such as the Public Service Commission Electronic Tariff System (ETS) or utility websites.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateSourceLastUpdated" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>the date at which a record or resource was last changed or modified in the source from which data was extracted.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateTimeDataExtract" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="en">the datetime at which data was extracted</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateTimeDataTransfer" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>the datetime at which data was transferred</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="updateFrequency">
                <xs:annotation>
                    <xs:documentation>The expected update frequency for this document</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="daily"/>
                        <xs:enumeration value="weekly"/>
                        <xs:enumeration value="monthly"/>
                        <xs:enumeration value="quarterly"/>
                        <xs:enumeration value="annually"/>
                        <xs:enumeration value="biannually"/>
                        <xs:enumeration value="multiyear"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MeasureType">
        <xs:sequence>
            <xs:element form="qualified" name="calculationMethod">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="avg"/>
                        <xs:enumeration value="by_billing_cycle"/>
                        <xs:enumeration value="by_metered_quantity"/>
                        <xs:enumeration value="by_variable_utility_settings"/>
                        <xs:enumeration value="fixed"/>
                        <xs:enumeration value="min"/>
                        <xs:enumeration value="max"/>
                        <xs:enumeration value="percentage"/>
                        <xs:enumeration value="sdv"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="calculationBaseValue"
                type="xs:string"/>
            <xs:element name="measurementSource" type="MeasurementKind"
                minOccurs="0"/>
            <xs:element minOccurs="0" ref="phases"/>
            <xs:element name="unitOfMeasure" type="UnitSymbolKind" minOccurs="0"/>
            <xs:element name="measurementInterval" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element ref="calculationMode"/>
                        <xs:element ref="calcIntervalKind"/>
                        <xs:element ref="calcIntervalPer"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" ref="statisticKind"/>
            <xs:element minOccurs="0" ref="inclusionStatus"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProfileMetricType">
        <xs:annotation>
            <xs:documentation xml:lang="en-US">Numeric cost model variable determined by a customer-specific scenario or utility setting.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="profileVariable" type="ProfileVariableKind"/>
            <xs:element minOccurs="0" name="profileVariableMeasurement"
                type="MeasureType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ScopeOfApplicabilityType">
        <xs:annotation>
            <xs:documentation>The scope of rate plan parameters to which a data point or set of data points is applicable. This will be used to filter charges data for the set applicable to a customer scenario to be calculated.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element form="qualified" name="include" type="ApplicabilityType">
                <xs:annotation>
                    <xs:documentation>A set of attribute values that constrain the scope of applicability of this data element by explicit inclusion. All other values for this same field are explicitly excluded.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="exclude" type="ApplicabilityType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A set of attribute values that constrain the scope of applicability of this data element by explicit exclusion. This allows for the exclusion of a subset of excluded values for one variables from a larger set of included values for another variable, e.g. "All service class 1 rate plans except time of use rate plans"</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RatePlanTerritoryType">
        <xs:sequence>
            <xs:element name="IedrRatePlanTerritoryId" type="xs:string"/>
            <xs:element name="territoryKind">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                        <xs:whiteSpace value="collapse"/>
                        <xs:enumeration value="system_operator_zone_id"/>
                        <xs:enumeration value="legal_division"/>
                        <xs:enumeration value="climate_zone_icc"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="geographicUnit" type="xs:string"/>
            <xs:element name="rawValue" type="xs:string"/>
            <xs:element ref="effectivePeriod"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TimeIntervalType">
        <xs:sequence>
            <xs:element form="qualified" name="startTime" type="xs:time"/>
            <xs:element name="endTime" form="qualified" type="xs:time"/>
        </xs:sequence>
    </xs:complexType>

    <!--
=======================================
Global Elements
=======================================
-->

    <xs:element name="calcIntervalKind">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="PER-CYCLE"/>
                <xs:enumeration value="CYCLE"/>
                <xs:enumeration value="DAY"/>
                <xs:enumeration value="WEEK"/>
                <xs:enumeration value="MONTH"/>
                <xs:enumeration value="YEAR"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="calcIntervalPer" type="xs:int"/>
    <xs:element name="calculationMode">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="TOTAL"/>
                <xs:enumeration value="PERIOD"/>
                <xs:enumeration value="SLIDING"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="commodity" type="CommodityKind"/>
    <xs:element name="customRatePlanAttribute">
        <xs:annotation>
            <xs:documentation>Flexible options for rate plan attributes by which to filter rate plans for applicability</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="attributeName" type="xs:string"/>
                <xs:element name="attributeValue" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="dataSource" type="DataSourceKind"/>
    <xs:element name="description">
        <xs:annotation>
            <xs:appinfo>description text</xs:appinfo>
            <xs:documentation>description text</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="2000"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="dpsCaseNumber" type="xs:string"/>
    <xs:element name="dpsPscNumber" type="xs:string"/>
    <xs:element name="dwellingScenario">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dwelling" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="dwelling" type="DwellingKind"/>
    <xs:element name="customerEntities">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="customerEntity" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="customerEntity" type="EntityKind"/>
    <xs:element name="effectivePeriod" type="DateIntervalType"/>
    <xs:element name="endUses">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="endUse" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="endUse" type="EndUseKind"/>
    <xs:element name="equipmentScenario">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="equipment" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="equipment" type="EquipmentKind"/>
    <xs:element name="hasMeasurementKinds">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">Measurement inputs to cost model</xs:appinfo>
            <xs:documentation xml:lang="en-US">Measurement inputs to cost, , e.g. "demand" for a rate plan that has demand charges</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="hasMeasurementKind" type="MeasurementKind"
                    maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="identifiers">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">the set of identifiers denoting the set of rate plans to which charges are (when used in include) or are not (when used in exclude) applicable</xs:appinfo>
            <xs:documentation xml:lang="en-US">The set of identifiers denoting the set of rate plans to which charges are (when used in include) or are not (when used in exclude) applicable. Multiple instances can exist for each identifier.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceClassReferences" minOccurs="0">
                    <xs:annotation>
                        <xs:appinfo>Applicable Service Classes</xs:appinfo>
                        <xs:documentation>Applicable service classes, eg. "1" for service Class 1. Use either an integer or "All" if applicable to all service classes.
                            If a suffix is added (ex.: "SC1TOU1" ) this field shall be populated with just the number for the parent service class. The suffix shall made available separately in "rateCodes"</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="serviceClassReference"
                                maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element form="qualified" name="rateReferences" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Applicable service rate codes, eg. "2" for Service Class 1 rate 2".</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="rateReference"
                                maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element form="qualified" minOccurs="0"
                    name="ratePlanReferences">
                    <xs:annotation>
                        <xs:documentation>Applicable ratePlanCodeUnique values. Not used when ConstrainVariableType is used within RatePlanType occurrences.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="ratePlanCodeUnique"
                                maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element form="qualified" minOccurs="0"
                    name="ratePlanModifierReferences">
                    <xs:annotation>
                        <xs:documentation>Applicable ratePlanModifierCodeUnique values</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded"
                                ref="ratePlanModifierCodeUnique"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="utilityTariffIdReferences" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="utilityTariffId"
                                maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:appinfo>utility-maintained identifier for a tariff or rate</xs:appinfo>
                                    <xs:documentation>utility-maintained identifier for a ta</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="iedrRatePlanBrowserURL" type="xs:anyURI">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">IEDR Permalink URL</xs:appinfo>
            <xs:documentation xml:lang="en-US">IEDR Permalink URL, ex.:https://iedr.nyserda.ny.gov/rate-plan/?rateId=KNYGSC22CT2</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="inclusionStatus" type="ValueInclusionKind"/>
    <xs:element name="initialEffectiveDate" type="xs:date"/>
    <xs:element name="mediaType">
        <xs:annotation>
            <xs:appinfo>media type of the source document</xs:appinfo>
            <xs:documentation>media type for document as registered by IANA, enumeration contains allowed subset.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="application/json">
                    <xs:annotation>
                        <xs:documentation>JSON</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="application/pdf">
                    <xs:annotation>
                        <xs:documentation>PDF</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="application/vnd.ms-excel">
                    <xs:annotation>
                        <xs:documentation>vnd.ms-excel</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration
                    value="application/vnd.oasis.opendocument.spreadsheet">
                    <xs:annotation>
                        <xs:documentation>vnd.oasis.opendocument.spreadsheet</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="application/vnd.oasis.opendocument.text">
                    <xs:annotation>
                        <xs:documentation>vnd.oasis.opendocument.text</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration
                    value="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
                    <xs:annotation>
                        <xs:documentation>vnd.openxmlformats-officedocument.spreadsheetml.sheet</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="application/zip">
                    <xs:annotation>
                        <xs:documentation>ZIP</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="image/gif">
                    <xs:annotation>
                        <xs:documentation>GIF</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="image/jpeg">
                    <xs:annotation>
                        <xs:documentation>JPEG</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="image/png">
                    <xs:annotation>
                        <xs:documentation>PNG</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="text/csv">
                    <xs:annotation>
                        <xs:documentation>CSV</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="text/html">
                    <xs:annotation>
                        <xs:documentation>HTML</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="text/plain">
                    <xs:annotation>
                        <xs:documentation>plain</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="text/rtf">
                    <xs:annotation>
                        <xs:documentation>RTF</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="text/xml">
                    <xs:annotation>
                        <xs:documentation>XML</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="month">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">an integer representing the calendar month (i.e. 1-12)</xs:appinfo>
            <xs:documentation xml:lang="en-US">an integer representing the calendar month(i.e. 1-12)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="1"/>
                <xs:maxInclusive value="12"/>
                <xs:totalDigits value="2"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="name" type="xs:string">
        <xs:annotation>
            <xs:appinfo xml:lang="en-US">data provider's own name for the data element</xs:appinfo>
            <xs:documentation xml:lang="en-US">data provider's own name for the data element</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="onSiteGenerationScenario">
        <xs:annotation>
            <xs:appinfo>Applicable onsite generation scenario</xs:appinfo>
            <xs:documentation/>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="onSiteGeneration" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="onSiteGeneration" type="OnSiteGenerationKind"/>
    <xs:element name="peakType">
        <xs:simpleType>
            <xs:annotation>
                <xs:documentation>Name of the Peak Type</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
                <xs:enumeration value="base"/>
                <xs:enumeration value="mid"/>
                <xs:enumeration value="peak"/>
                <xs:enumeration value="offpeak"/>
                <xs:enumeration value="shoulderpeak"/>
                <xs:enumeration value="superpeak"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="phases">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="phase" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="phase" type="PhaseCodeKind">
        <xs:annotation>
            <xs:appinfo xml:lang="en">Phase code</xs:appinfo>
            <xs:documentation xml:lang="en">Phase code value representing phases. from CIM: "Number of wires and specific nominal phases can be deduced from enumeration literal values. For example, ABCN is three-phase, four-wire, s12n (splitSecondary12N) is single-phase, three-wire, and s1n and s2n are single-phase, two-wire."</xs:documentation>
            <xs:documentation>source: CIM TC57: https://ontology.tno.nl/IEC_CIM/cim_UsagePoint.phaseCode.html</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="profile">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="customerClasses" minOccurs="0">
                    <xs:annotation>
                        <xs:appinfo>Applicable customer classes</xs:appinfo>
                        <xs:documentation>Applicable customer classes</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded"
                                name="customerClass" type="CustomerClassKind"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element ref="customerEntities" minOccurs="0"/>
                <xs:element ref="dwellingScenario" minOccurs="0"/>
                <xs:element minOccurs="0" ref="endUses">
                    <xs:annotation>
                        <xs:appinfo>Applicable end uses</xs:appinfo>
                        <xs:documentation/>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="equipmentScenario"/>
                <xs:element minOccurs="0" ref="onSiteGenerationScenario"/>
                <xs:element minOccurs="0" ref="phases"/>
                <xs:element minOccurs="0" ref="serviceProvisionScenario"/>
                <xs:element minOccurs="0" ref="ratePlanTerritories"/>
                <xs:element minOccurs="0" ref="voltages"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="provenanceMetadata">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="tariffBookLeafRanges"/>
                <xs:element maxOccurs="unbounded" ref="dpsCaseNumber"/>
                <xs:element ref="iedrRatePlanBrowserURL" minOccurs="0">
                    <xs:annotation>
